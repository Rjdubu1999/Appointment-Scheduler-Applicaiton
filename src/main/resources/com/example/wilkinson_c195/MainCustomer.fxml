<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="customerAnchorPane" prefHeight="459.0" prefWidth="627.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.MainCustomerController">
   <children>
      <Label layoutX="258.0" layoutY="28.0" text="Customers">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TableView fx:id="customerTableView" layoutX="131.0" layoutY="71.0" prefHeight="297.0" prefWidth="372.0">
        <columns>
          <TableColumn fx:id="customerIdCol" prefWidth="112.0" text="ID" />
          <TableColumn fx:id="customerNamecol" prefWidth="259.0" text="Customer" />
        </columns>
      </TableView>
      <ButtonBar layoutX="129.0" layoutY="387.0" prefHeight="40.0" prefWidth="353.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#onActionAdd" style="-fx-background-radius: 20;" text="Add" />
            <Button mnemonicParsing="false" onAction="#onActionModify" style="-fx-background-radius: 20;" text="Modify" />
            <Button mnemonicParsing="false" onAction="#onActionDelete" style="-fx-background-radius: 20;" text="Delete" />
            <Button mnemonicParsing="false" onAction="#onActionBack" style="-fx-background-radius: 20;" text="Back" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
